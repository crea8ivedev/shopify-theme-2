{{ 'component-social-icons.css' | asset_url | stylesheet_tag }}
<ul class="social-icon-list{% if class %} {{ class }}{% endif %}" role="list">
  {%- if settings.instagram_account != blank -%}
    <li class="social-icon-item">
      <a href="{{ settings.instagram_account }}" class="link social-icon-link icon-instagram">
        {%- render 'icon-instagram' -%}
        <span class="visually-hidden">{{ 'general.social.links.instagram' | t }}</span>
      </a>
    </li>
  {%- endif -%}
